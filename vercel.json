{
  "version": 2,
  "functions": {
    "api/message.js": { "runtime": "nodejs18.x" },
    "api/webhook.js": { "runtime": "nodejs18.x" }
  },
  "routes": [
    { "src": "/api/message", "dest": "/api/message.js" },
    { "src": "/api/webhook", "dest": "/api/webhook.js" }
  ],
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "outputDirectory": ""
}
